<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="FloatingActionButton">
        <attr name="fab_colorPressed" format="color" />
        <attr name="fab_colorNormal" format="color" />
        <attr name="fab_colorRipple" format="color" />
        <attr name="fab_shadow" format="boolean" />
        <attr name="fab_type" format="enum">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FloatingActionsMenu" parent="FloatingActionButton">
        <attr name="fab_menuButtonSelectedSrc" format="reference" />
        <attr name="fab_menuButtonUnSelectedSrc" format="reference" />
        <attr name="fab_menuButtonColorPressed" format="color" />
        <attr name="fab_menuButtonColorNormal" format="color" />
        <attr name="fab_menuButtonColorSelected" format="color" />
        <attr name="fab_menuButtonColorRipple" format="color" />
        <attr name="fab_expandDirection" format="enum">
            <enum name="up" value="0" />
            <enum name="down" value="1" />
        </attr>
    </declare-styleable>
    
	<declare-styleable name="DoubleRingProgress">
        <attr name="arc_angle" format="float" />
        <attr name="arc_max" format="integer" />
        <attr name="stroke_width_outter" format="dimension" />
        <attr name="progress_outter" format="integer" />
        <attr name="unfinished_color_outter" format="color" />
        <attr name="finished_color_outter" format="color" />
        <attr name="stroke_width_inner" format="dimension" />
        <attr name="progress_inner" format="integer" />
        <attr name="unfinished_color_inner" format="color" />
        <attr name="finished_color_inner" format="color" />
        <attr name="fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="BoundaryBounceViewPager">
        <!--
        determines the maximum amount of translation along the z-axis during the overscroll.
        Default is 150.
        -->
        <attr name="overscroll_translation" format="dimension" />

        <!-- Duration of animation when user releases the over scroll. Default is 400 ms. -->
        <attr name="overscroll_animation_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="CircleWaveProgressBar">
        <attr name="above_wave_color" format="color"/>
        <attr name="below_wave_color" format="color"/>
        <attr name="progress" format="integer"/>
        <attr name="wave_height" format="dimension"/>
        <attr name="wave_width_rate" format="enum">
            <enum name="large" value="3"/>
            <enum name="middle" value="2" />
            <enum name="small" value="1" />
        </attr>
        <attr name="wave_hz" format="enum">
            <enum name="fast" value="15" />
            <enum name="normal" value="10" />
            <enum name="slow" value="5" />
            <enum name="soslow" value="2"/>
            <enum name="tooslow" value="1"/>
        </attr>
        <attr name="progressTextSize" format="dimension"/>
        <attr name="progressTextColor" format="color"/>
        <attr name="progressTextVisible" format="boolean"/>
        <attr name="wavestrokeWidth" format="dimension"/>
        <attr name="wavestrokeColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="Themes">
        <attr name="waveViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="RoundedImageView">
        <attr name="riv_corner_radius" format="dimension" />
        <attr name="riv_corner_radius_top_left" format="dimension" />
        <attr name="riv_corner_radius_top_right" format="dimension" />
        <attr name="riv_corner_radius_bottom_left" format="dimension" />
        <attr name="riv_corner_radius_bottom_right" format="dimension" />
        <attr name="riv_border_width" format="dimension" />
        <attr name="riv_border_color" format="color" />
        <attr name="riv_mutate_background" format="boolean" />
        <attr name="riv_oval" format="boolean" />
        <attr name="android:scaleType" />
        <attr name="riv_tile_mode">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_x">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_y">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="LD_WaveView">
        <attr name="wave_color" format="color"></attr>
        <attr name="wave_circle" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PolygonChart">
        <attr name="textPadding" format="dimension"/>
        <attr name="max_value" format="integer"/>
        <attr name="side_num" format="integer"/>
        <attr name="border_color" format="color"/>
        <attr name="border_size" format="dimension"/>
        <attr name="text_size" format="dimension"/>
        <attr name="text_color" format="color"/>
        <attr name="solid_color" format="color"/>
        <attr name="dash_color" format="color"/>
    </declare-styleable>
</resources>